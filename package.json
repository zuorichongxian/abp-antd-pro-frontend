{
  "name": "ant-design-pro",
  "version": "6.0.0-beta.1",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "max build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "lint": "npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "openapi": "max openapi",
    "playwright": "playwright install && playwright test",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev max dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev max dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev max dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev max dev",
    "test:e2e": "node ./tests/run-tests.js",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/compatible": "^1.1.2",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-components": "^2.0.0",
    "@antv/data-set": "^0.11.8",
    "@icon-park/react": "^1.4.2",
    "@umijs/route-utils": "^2.1.3",
    "ahooks": "^3.7.1",
    "antd": "^4.20.0",
    "axios": "^1.1.2",
    "bizcharts": "^4.1.20",
    "bizcharts-plugin-slider": "^3.0.1",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.5",
    "dva": "^2.4.1",
    "dva-model-extend": "^0.1.2",
    "echarts-for-react": "^3.0.2",
    "esri-loader": "^3.6.0",
    "file-saver": "^2.0.5",
    "gg-editor": "^2.0.0",
    "lodash": "^4.17.21",
    "lodash-decorators": "^6.0.1",
    "moment": "^2.29.0",
    "numeral": "^2.0.6",
    "nzh": "^1.0.8",
    "omit.js": "^2.0.2",
    "qs": "^6.11.0",
    "rc-animate": "^3.1.1",
    "rc-menu": "^9.1.0",
    "rc-util": "^5.16.0",
    "react": "^17.0.0",
    "react-chat-elements": "^12.0.5",
    "react-color": "^2.19.3",
    "react-container-query": "^0.12.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dev-inspector": "^1.7.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^17.0.0",
    "react-fittext": "^1.0.0",
    "react-helmet-async": "^1.2.0",
    "react-image-crop": "^10.0.8",
    "react-media": "^1.10.0",
    "react-router-cache-route": "^1.12.11",
    "react-use": "^17.4.0",
    "store": "^2.0.12",
    "viewerjs": "^1.10.5"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.0",
    "@playwright/test": "^1.17.0",
    "@types/classnames": "^2.3.1",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.0",
    "@types/lodash": "^4.14.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@umijs/fabric": "^2.11.1",
    "@umijs/max": "^4.0.0-rc.22",
    "@umijs/openapi": "^1.3.0",
    "babel-plugin-import": "^1.13.5",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.0",
    "eslint": "^7.32.0",
    "gh-pages": "^3.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.5.0",
    "swagger-ui-dist": "^4.12.0",
    "tailwindcss": "^3",
    "typescript": "^4.5.0",
    "umi-presets-pro": "^1.0.1",
    "umi-serve": "^1.9.10"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
